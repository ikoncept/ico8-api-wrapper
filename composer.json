{
    "name": "ikoncept/ico8-api-wrapper",
    "description": "API wrapper for Icat portals",
    "license": "proprietary",
    "type": "library",
    "authors": [
        {
            "name": "Albin N",
            "email": "albin@myrmalm.se"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/database": "^7.0|^8.40.0|^9.0",
        "illuminate/support": "^7.0.5|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.3",
        "orchestra/testbench": "^6.4",
        "nunomaduro/collision": "^5.1",
        "nunomaduro/larastan": "^0.6.10"
    },
    "autoload": {
        "psr-4": {
            "Ikoncept\\Ico8\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ikoncept\\Ico8\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ikoncept\\Ico8\\Ico8ServiceProvider"
            ],
            "aliases": {
                "Ico8": "Ikoncept\\Ico8\\Facades\\Ico8"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
